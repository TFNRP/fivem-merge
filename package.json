{
  "name": "fivem-merge",
  "version": "0.0.6",
  "description": "An API to merge your FiveM vehicle resources",
  "main": "./src/index.js",
  "module": "./src/index.mjs",
  "types": "./typings/index.d.ts",
  "files": [
    "src",
    "cli"
  ],
  "exports": {
    "./*": "./*",
    ".": {
      "require": "./src/index.js",
      "import": "./src/index.mjs"
    }
  },
  "bin": {
    "vmerge": "./cli/index.js"
  },
  "scripts": {
    "test": "npm run lint && npm run lint:typings",
    "test:typescript": "tsc",
    "lint": "eslint src cli",
    "lint:fix": "eslint src cli --fix",
    "lint:typings": "tslint typings/index.d.ts",
    "prettier": "prettier --write src/**/*.js typings/**/*.ts cli/**/*.js",
    "prepublishOnly": "npm run test && gen-esm-wrapper ./src/index.js ./src/index.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TFNRP/fivem-merge.git"
  },
  "keywords": [
    "fivem",
    "api",
    "merge",
    "manager",
    "asset"
  ],
  "author": "Reece Stokes <hagen@paw.bot>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/TFNRP/fivem-merge/issues"
  },
  "homepage": "https://github.com/TFNRP/fivem-merge#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "eslint-plugin-html": "^6.2.0",
    "fast-xml-parser": "^3.20.3",
    "lodash": "^4.17.21",
    "luaparse": "^0.3.1",
    "md5-dir": "^0.2.0",
    "mri": "^1.2.0",
    "mv": "^2.1.1",
    "xml-formatter": "^2.4.1"
  },
  "devDependencies": {
    "dtslint": "^4.1.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "gen-esm-wrapper": "^1.1.3",
    "prettier": "^2.4.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
